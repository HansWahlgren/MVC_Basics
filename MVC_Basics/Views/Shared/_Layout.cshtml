<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewData["Title"] - MVC_Basics</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/css/StyleSheet.css" rel="stylesheet" />
    @*<title>@ViewBag.Title</title>*@
</head>
<body>
    <header>
        <a asp-action="Index" asp-controller="Home"><img id="logotype" src="~/img/wahlgren_logo.png" alt="Wahlgren"></a>
        <nav id="main_navigation_bar">
            <ul>
                <li>
                    <a asp-action="Index" asp-controller="Home">Home</a>
                </li>
                <li>
                    <a asp-action="Contact" asp-controller="Home">Contact</a>
                </li>
                <li>
                    <a asp-action="Projects" asp-controller="Home">Projects</a>
                </li>
                <li>
                    <a asp-action="Index" asp-controller="People">People</a>
                </li>
            </ul>
        </nav>
    </header>

    <hr />

    <div id="view_div">
        @RenderBody()
    </div>

    <hr />

    <footer>
        <p>&copy;Philip Wahlgren @DateTime.Now.ToShortDateString()</p>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
